@model IEnumerable<MichellesWebsite.Models.EnquiryListView>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.user.FullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EnquiryId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.queryType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ts)
        </th>
        <th>
            Options
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(model => item.user.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EnquiryId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.queryType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ts)
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { id=item.EnquiryId }) |
            @Html.ActionLink("Details", "Details", new { id = item.EnquiryId })
        </td>
    </tr>
}
</table>
<p>
    @Html.ActionLink("Back to Account", "Index", "Manage", null)
</p>